{
  "name": "@wibr/template-reusable-component",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/template-reusable-component.umd.js",
  "module": "./dist/template-reusable-component.es.js",
  "exports": {
    ".": {
      "import": "./dist/template-reusable-component.es.js",
      "require": "./dist/template-reusable-component.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:nowatch": "vitest run .",
    "test:coverage": "vitest run --coverage",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "stylelint": "stylelint '**/*.css'"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@volar-plugins/vetur": "latest",
    "@vue/test-utils": "^2.2.10",
    "@wibrc/eslint-config-vue": "github:IT-WIBRC/wibrc-eslint-config-vue",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.0",
    "jsdom": "21.1.0",
    "lint-staged": "12.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint"
    ],
    "*.{css,vue}": [
      "stylelint"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
